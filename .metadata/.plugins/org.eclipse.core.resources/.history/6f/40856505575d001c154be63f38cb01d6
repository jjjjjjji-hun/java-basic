package game.inheritance;

public class Archer extends Commoner{
	private int lDam;
	
	public Archer(String name) {
		super(name);
		this.lDam = 3;
	}

	public void getInfo() {
		System.out.println("레벨 : " + lv + ", 체력 : " + hp + 
				           ", 마나 : " + mp + ",  아이디 : " + name + 
				           ", 원거리데미지 : " + lDam);
    }
}