package inheritance;

public class Main02P254 {

	public static void main(String[] args) {
		Student st1 = new Student();
		st1.name = "김자바";
		st1.age = 55;
		st1.stuNum = 1511012;
		
		st1.getInfo();
		st1.getStudentInfo();
		
		Salaryman m1 = new Salaryman();
		m1.name = "김지훈";
		m1.age = 26;
		m1.salary = 100000000;
		
		m1.getInfo();
		m1.getSalaryInfo();

	}

}
