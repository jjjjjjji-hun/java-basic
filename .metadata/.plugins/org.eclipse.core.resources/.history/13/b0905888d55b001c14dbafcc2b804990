package accessmodifier;

public class MainP197 {

	public static void main(String[] args) {
		// 전사를 하나 만들고 사냥을 3번 하세요.
		
		Warrior w1 = new Warrior("타락파워전사");
		w1.hp = 100000;
		w1.hunt();
		w1.hunt();
		w1.hunt();
		w1.hunt();
		w1.hunt();
		w1.hunt();
		w1.healing();
		w1.hunt();
		w1.hunt();
		w1.hunt();
		w1.hunt();
		w1.levelUp();

	}

}
